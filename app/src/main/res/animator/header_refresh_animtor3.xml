<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
     android:ordering="sequentially">
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M120,60 L180,60"
        android:valueTo="M20,150 L180,150"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,150 L180,150"
        android:valueTo="M20,150 L80,150"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,150 L80,150"
        android:valueTo="M20,60 L180,60"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,60 L180,60"
        android:valueTo="M120,60 L180,60"
        android:valueType="pathType"
        />
</set>