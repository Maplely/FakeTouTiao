<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
     android:ordering="sequentially">
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M120,30 L180,30"
        android:valueTo="M20,120 L180,120"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,120 L180,120"
        android:valueTo="M20,120 L80,120"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,120 L80,120"
        android:valueTo="M20,30 L180,30"
        android:valueType="pathType"
        />
    <objectAnimator
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:duration="400"
        android:propertyName="pathData"
        android:valueFrom="M20,30 L180,30"
        android:valueTo="M120,30 L180,30"
        android:valueType="pathType"
        />
</set>