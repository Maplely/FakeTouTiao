<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/category_item_corner" />
            <stroke android:width="0.5dip" android:color="@color/ssxinxian1_disable" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/category_item_corner" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="0.5dip" android:color="@color/ssxinxian2_press" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/category_item_corner" />
            <stroke android:width="0.5dip" android:color="@color/ssxinxian2" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>