<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/appad_v18_button_success_pressed_stroke" />
            <solid android:color="@color/appad_v18_button_success_pressed_bg" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dip" android:color="@color/appad_v18_button_success_normal_stroke" />
            <solid android:color="@color/appad_v18_button_success_normal_bg" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
</selector>