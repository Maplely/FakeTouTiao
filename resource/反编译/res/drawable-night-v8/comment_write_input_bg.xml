<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0dip" android:color="#ff444444" />
            <solid android:color="#ff1f2022" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="1.0dip" android:color="#ff444444" />
            <solid android:color="#ff1f2022" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0dip" android:color="#ff444444" />
            <solid android:color="#ff1a1b1d" />
        </shape>
    </item>
</selector>