<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.0dip" />
            <gradient android:startColor="@color/detail_more_title_seekbar_bg" android:endColor="@color/detail_more_title_seekbar_bg" android:angle="270.0" android:centerY="0.5" android:centerColor="@color/detail_more_title_seekbar_bg" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <gradient android:startColor="@color/detail_more_title_seekbar_progress" android:endColor="@color/detail_more_title_seekbar_progress" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>